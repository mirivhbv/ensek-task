{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ApiSettings": {
    "GenericBoundaries": {
      "Minimum": 0,
      "Maximum": 1000,
      "Regex": "^[0-9]{1,3}$"
    },
    "DatabaseSettings": {
      "DbType": "Npgsql",
      "ConnectionStrings": "Host=db;Port=5432;Database=ensekdb;Username=ensek;Password=ensekpass"
    },
    "FixedWindowRateLimit": {
      "AutoReplenishment": true,
      "PermitLimit": 40,
      "QueueLimit": 2,
      "Window": "00:02:00",
      "QueueProcessingOrder": "OldestFirst"
    }
  },
  "ApiDocument": {
    "Servers": [
      {
        "Url": "https://localhost:5000/v1"
      }
    ],
    "Info": {
      "Title": "Meter Reading API",
      "Version": "v1",
      "Description": "API for managing meter readings."
    }
  }
}